{
    "name": "homchen/hierarchy-profit",
    "type": "library",
    "authors": [
        {
            "name": "Hom Chen",
            "email": "homchen@mail.local"
        }
    ],
    "require": {
        "laravel/framework": "5.6.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Hierarchy\\": "src/"
        },
        "files": [
            "src/Supports/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Hierarchy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hierarchy\\HierarchyServiceProvider"
            ],
            "aliases": {
                "HProfit": "Hierarchy\\Supports\\Facades\\HProfit"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
